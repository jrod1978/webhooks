{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["POST"],
      "dest": "/api/webhook/index.js"
    },
    {
      "src": "/api/webhook/test",
      "methods": ["GET"],
      "dest": "/api/webhook/test.js"
    }
  ]
}

